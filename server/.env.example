# Receipt server (Express)
PORT=5174

# Gmail OAuth (required to connect Gmail)
# Create in Google Cloud Console -> OAuth Client ID
# Use redirect URI: http://localhost:5174/api/gmail/oauth2callback
GMAIL_CLIENT_ID=your_gmail_oauth_client_id
GMAIL_CLIENT_SECRET=your_gmail_oauth_client_secret
GMAIL_REDIRECT_URI=http://localhost:5174/api/gmail/oauth2callback
